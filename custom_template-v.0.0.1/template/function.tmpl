        type args struct {

		{{- range .TestParameters}}
			{{Param .}} {{.Type}}		
		{{- end}}
	 }


       test:=args{
                //pass the parameter here
                
       }
       {{- end}}
       
       
       {{- with .Receiver}}
		{{- if .IsStruct}}
		         // Add External package name in ____
	       	{{Receiver .}} := {{if .Type.IsStar}}&{{end}}_____.{{.Type.Value}}{}
		{{- end}}
	{{- end}}


	{{- if and (not .OnlyReturnsError) (not .OnlyReturnsOneValue)}}
